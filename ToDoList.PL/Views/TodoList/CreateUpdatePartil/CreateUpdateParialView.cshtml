@model TaskViewModel
@{
    var Objectss = new SelectList( @ViewBag.Object, "iDd", "ObjectaName");
}
<input asp-for="imageName" hidden>
<div class="form-row">
    <div class="form-group col-md-12">
        <label asp-for="TaskName">Enter The Task Name</label>
        <input asp-for="TaskName" class="form-control" placeholder="Task Name">
    </div>
  
    <div class="form-group col-md-12">
        <label asp-for="TaskDescreption">Task Descreption</label>
        <input asp-for="TaskDescreption" class="form-control" placeholder="Task Descreption">
    </div>
    <div class="form-group col-md-12">
        <label asp-for="formFile"></label>
        <input asp-for="formFile" class="form-control">
    </div>
    <div class="form-group col-md-12">
        <label class="col-form-label" asp-for="Object"></label>
        <select class="form-control" asp-for="ObjectFK" asp-items="@Objectss">
            <option value="@null">--Select--</option>
        </select>
    </div>
    
    <div class="form-group col-md-12">
        <label asp-for="TimeEndTheTask">Time End The Task</label>
        <input asp-for="TimeEndTheTask" class="form-control">
    </div>
</div>
