@model Objects
@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<form asp-action="Edit">
    <div asp-validation-summary="All"></div>
    <div class="form-group">
        <label class="col-form-label" asp-for="ObjectaName"></label>
        <input asp-for="ObjectaName" class="form-control" placeholder="Enter Object Name">
        <span asp-validation-for="ObjectaName" class="text-danger"></span>
    </div>
    <input type="submit" value="Save" class="btn btn-primary"> |
    <a asp-action="Index" type="submit" class="btn btn-info">Back</a>

</form>
