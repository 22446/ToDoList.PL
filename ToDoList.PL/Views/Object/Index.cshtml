@model IEnumerable<Objects>;
@{
    ViewData["Title"] = "Index";
}



<a asp-controller="Object" asp-action="Create" class="btn btn-info">Create new Object</a>

<br>
<hr>
@if (Model.Count() > 0)
{
    <h1>All Objects</h1>
    <br>
    <table class="table table-hover">
        <thead>
            <tr>
                <th scope="col">@Html.DisplayNameFor(a=>a.ObjectaName)</th>
               
               
                <th scope="col">Edit</th>
                <th scope="col">Delete</th>
            </tr>
        </thead>
        <tbody>

            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.ObjectaName</td>

                    <partial name="NewFolder/CCommon", model="@item.iDd">
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <div class="alert alert-warning" role="alert">
        <h4 class="alert-heading">There is no Objects !</h4>
    </div>
}