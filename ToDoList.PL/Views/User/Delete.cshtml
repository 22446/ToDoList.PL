@model UserViewModel
@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Delete</h1>

<form asp-action="DeleteConfirm" , enctype="multipart/form-data">
    <div class="form-row">

        <div class="form-group col-md-12">
            <label asp-for="Id">ID</label>
            <input asp-for="Id" class="form-control" readonly>
        </div>
        <div class="form-group col-md-12">
            <label asp-for="Fname">First Name</label>
            <input asp-for="Fname" class="form-control" readonly>
        </div>
        <div class="form-group col-md-12">
            <label asp-for="Lname"></label>
            <input asp-for="Lname" class="form-control " readonly>
        </div>

        <div class="form-group col-md-12">
            <label asp-for="PhoneNumber">Phone Number</label>
            <input asp-for="PhoneNumber" class="form-control" readonly>
        </div>
        <div class="form-group col-md-12">
            <label asp-for="Email">Email</label>
            <input asp-for="Email" class="form-control" readonly>
        </div>
        <input type="submit" value="Delete"  class="btn btn-danger"> |
        <button asp-action="Index" class="btn btn-info">Back</button>
    </div>
</form>